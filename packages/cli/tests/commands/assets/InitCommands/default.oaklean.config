{
	// Inherits configuration settings from another config file
	"extends": ".oaklean.local",
	"exportOptions": {
		// Output directory for profiles and reports
		"outDir": "profiles/",
		// Timestamped historical reports
		"outHistoryDir": "profiles_history/",
		// Project source root
		"rootDir": "./",
		// Export .cpuprofile for external tools
		"exportV8Profile": false,
		// Export .oak file for the OakleanVSCode Extension
		"exportReport": true,
		// Export raw power sensor data (.mcollection files)
		"exportSensorInterfaceData": false
	},
	"projectOptions": {
		// A unique UUID used to identify this project within the (Oaklean) ecosystem
		"identifier": "7cc8cf7f-3cc9-49f8-8f06-3c3929aa004d"
	},
	"runtimeOptions": {
		"seeds": {},
		"v8": {
			"cpu": {
				// Sample rate in Âµs (lower = higher detail/overhead)
				"sampleInterval": 1
			}
		}
	}
}